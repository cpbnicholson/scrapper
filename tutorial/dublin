<script type="text/javascript">
	
    

	$('#Region').regionLocalityDropdown({ target: '#Localities', url: '/api/localities/prioritiseddecendants/' });


	$(".gallery").each(function () {

		var currentGallery = this;

		$(".arrowRight", currentGallery).click(function () {

			var visibleImage = $(".galleryImage:visible", currentGallery);
			$("img", visibleImage).hide();

			var nextImage = $(visibleImage).next(".galleryImage");

			if (nextImage.length != 0) {
				$("img", nextImage).show();
			} else {
				$("img:first", currentGallery).show();
			}
		});

		$(".arrowLeft", currentGallery).click(function () {

			var visibleImage = $(".galleryImage:visible", currentGallery);
			$("img", visibleImage).hide();

			var nextImage = $(visibleImage).prev(".galleryImage");

			if (nextImage.length != 0) {
				$("img", nextImage).show();
			} else {
				$("img:last", currentGallery).show();
			}
		});

	});


	$(".premium").hover(function () {
		$(this).addClass("hover");
	}, function () {
		$(this).removeClass("hover");
	});

	$(".slideshowLink").click(function () {
		var id = $(this).attr("id");
		$("a[rel='" + id + "']").colorbox({
			open: true,
			opacity: 0.2,
			current: "Image {current} of {total}"
		});
	});

	$(document).ready(function () {

		var hintText = "To get the best response fill in some detail..... Do you need advice about mortgages? Is a garden a must? What's the minimum number of bedrooms you need?";

		$(".contactAllTextarea")
			.addClass("hintText")
			.val(hintText)
			.focus(function () {
				if ($(this).val() == hintText) {
					$(this)
						.val("")
						.removeClass("hintText");
				}
			})
			.focusout(function () {
				if ($(this).val() == "") {
					$(this)
						.val(hintText)
						.addClass("hintText");
				}
			});



		$(".contactAllList #selectAll").click(function () {
			if ($(this).text() == "Select All") {
				$(this).text("Deselect All");
				$("input[type=checkbox]", ".contactAllList").attr("checked", true);
				$(".contactAllList li").addClass("selected");
			} else {
				$(this).text("Select All");
				$("input[type=checkbox]", ".contactAllList").attr("checked", false);
				$(".contactAllList li").removeClass("selected");
			}

		});

		$("#contactAllSubmit").live("click", function (e) {

			if ($(".contactAllTextarea").hasClass("hintText")) {
				$(".contactAllTextarea").val("").removeClass("hintText");
			}

			$('#contactAllForm').validate({
				rules: {
					Email: { email: true },
					Phone: { phone: true }
				},
				messages: {
					Name: "Please enter your name",
					Email: "Please enter your email address",
					Phone: "Please enter your phone number",
					Message: "Please enter a message",
					UserType: "Please select one"
				},
				showErrors: function (errorMap, errorList) {
					this.defaultShowErrors();
					$('#contactAllErrorMessages').hide();
				},
				errorPlacement: function (error, element) {
					error.insertBefore(element);
				}
			});

			var groupIDs = new Array();
			$("input:checked").each(function (i) {
				var groupID = parseInt($(this).attr("name"));
				if (!isNaN(groupID))
					groupIDs.push(groupID);
			});
			var agents = groupIDs.join(',');

			var isAgentsValid = true;
			if (agents == "" || agents == null) {
				$("#contactAllListErrors").html("<span>You have not selected any agents. Please select at least one agent from the list</span>");
				isAgentsValid = false;
			} else if (groupIDs.length > 30) {
				$("#contactAllListErrors").html("<span>You've selected " + groupIDs.length + " agents. Please select maximum 30 agents from the list</span>");
				isAgentsValid = false;
			} else {
				$("#contactAllListErrors").html("");
			}

			if ($('#contactAllForm').valid()) {
				if (!isAgentsValid) { return false; }

				$("#contactAllSubmit").text("Sending ...");
				$("#contactAllSubmit").attr("disabled", "disabled");
				$("#Agents").val(agents);
				$.ajax({
					type: 'POST',
					url: $("#contactAllForm").attr('action'),
					data: $("#contactAllForm").serialize(),
					dataType: 'html',
					success: function (data) {
						$('#contactAllPanel').html(data);
					},
					error: function () { alert('An error occurred'); }
				});
			}
			return false;
		});

		if ($("#contactAllPanel").exists()) {

			$(".contactAllList li .main").click(function () {
				var li = $(this).parent();
				$(li).toggleClass("selected");
				$("input[type=checkbox]", li).attr("checked", !$("input[type = checkbox]", li).attr("checked"));
			});

			$(".contactAllList input[type=checkbox]").click(function () {
				$(this).parent().parent().toggleClass("selected");
			});
		}


		// Property Alerts
		$("#refineInputs select").change(function () {
			disableCreatePropertyAlert();
		});
		$("#refineInputs input").keyup(function () {
			disableCreatePropertyAlert();
		});

		function disableCreatePropertyAlert() {
			$("#propertyAlert").unbind();
			$("#propertyAlert").removeClass().addClass("disabled");
		}

		$("#name").focus();

		var name = $("h1.refine").html();
		$("#name").val(name);

		$('#saveButton').live('click', function () {
			var frequency = $(".saveSearchForm #frequency").val();
			$.ajax({
				type: "POST",
				url: "/user/savesearch",
				data: {
					Url: document.URL,
					Frequency: frequency
				},
				success: function (data) {
					$('.saveSearchForm').html("<span class='saveConfirmation'>Your Property Alert was saved.</span><a href='/user#propertyAlerts' class='manageSearches'>Manage Your Property Alerts >></a><a class='confirmButton button green main'>Ok</a>");
					$("#propertyAlert").addClass("created");
					$("#propertyAlert span a").text("Property Alert Created");
				}
			});

			return false;
		});

		$("#cancel").live("click", function () {
			$("#createPropertyAlert").slideUp();
		});

		$(".confirmButton").live("click", function () {
			$("#createPropertyAlert").slideUp();
		});
		/////////////////////////////////

	});

</script>
        

